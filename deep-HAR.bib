@Article{Ord2016Deep,
  author  = {Ordóñez, F. J. and Roggen, D},
  title   = {Deep Convolutional and LSTM Recurrent Neural Networks for Multimodal Wearable Activity Recognition},
  journal = {Sensors},
  year    = {2016},
  volume  = {16},
  number  = {1},
  pages   = {115},
  comment = {Similarly, Ordóñez and Roggen (2016) proposed a convolutional neural network and long short term memory to automatically learn translational invariant features and model temporal dependencies in multimodal sensor comprise of accelerometer and gyroscope sensor. The pooling layer in the network was replaced with a recurrent layer (LSTM) that models the temporal sequence, whereas the final layer is the SoftMax regression that produces the class prediction. The technique was compared with baseline CNN using OPPORTUNITY and Skoda datasets with 0.61F1 score performance.

同样，Ordonez和Roggen(2016)提出了一种卷积神经网络和长短期记忆的方法，在由加速度计和陀螺仪传感器组成的多模态传感器中自动学习平移不变特征和建模时间依赖性。
网络中的池化层被建模时间序列的递归层(LSTM)所替代，而最后一层是产生类预测的SoftMax回归。
将该技术与使用OPPORTUNITY和Skoda数据集的基线CNN进行比较，得分为0.61F1。

In [15], the authors propose a generic deep framework for activity recognition based on convolutional and long short-term memory (LSTM) recurrent units.},
}

@InProceedings{Morales2016Deep,
  author    = {Morales, Francisco Javier Ordóñez and Roggen, Daniel},
  title     = {Deep convolutional feature transfer across mobile activity recognition domains, sensor modalities and locations},
  booktitle = {Acm International Symposium on Wearable Computers},
  year      = {2016},
  comment   = {To develop a multi-fusion architecture of CNN and LSTM, Morales and Roggen (2016) examined the effect of transfer learning at the network kernel between users, applications domains, sensor modalities and sensor placements in human activity recognition. They noted that transfer learning greatly reduced training time and are sensitive to sensor characteristics, placement and motion dynamic. They utilised the above automatic feature representation method to develop a hybrid of CNN and LSTM for extraction of robust features for human activity recognition in a wearable device.

Morales和Roggen(2016)为了开发CNN和LSTM的多融合架构，研究了在人类活动识别中，用户、应用领域、传感器模式和传感器位置之间的网络内核传输学习的效果。
他们指出，转移学习大大减少了训练时间，并且对传感器的特性、位置和动态运动非常敏感。
他们利用上述自动特征表示方法，将CNN和LSTM相结合，提取出鲁棒特征，用于可穿戴设备的人类活动识别。},
}

@Article{Yao2016DeepSense,
  author  = {Yao, Shuochao and Hu, Shaohan and Zhao, Yiran and Zhang, Aston and Abdelzaher, Tarek},
  title   = {DeepSense: A Unified Deep Learning Framework for Time-Series Mobile Sensing Data Processing},
  year    = {2016},
  comment = {Alternatively, a convolutional neural network with Gated Recurrent Unit (GRU) was proposed by S. Yao, Hu, Zhao, Zhang, and Abdelzaher (2016) for activity recognition and car tracking using accelerometer, gyroscope and magnetometer data. CNN and GRU were integrated to extract local interaction among identical mobile sensor, merged into global interaction and then extract temporal interaction to model signal dynamics.

另外，S. Yao，胡，Zhao, Zhang, Abdelzaher(2016)利用加速度计、陀螺仪、磁力计数据提出了一种基于门控回归单元的卷积神经网络(tional neural network with gate Unit, GRU)，用于活动识别和车辆跟踪。
融合CNN和GRU提取相同移动传感器之间的局部交互，融合到全局交互中，提取时间交互，对信号动力学建模。},
}

@Article{alzantot2017sensegen:,
  author  = {Alzantot, Moustafa and Chakraborty, Supriyo and Srivastava, Mani B},
  title   = {SenseGen: A deep learning architecture for synthetic sensor data generation},
  journal = {pervasive computing and communications},
  year    = {2017},
  pages   = {188--193},
  comment = {In recent work, Alzantot, Chakraborty, and Srivastava (2017) explored generation of artificial activity data by fusion of mixture density network and long short term memory. The approach was proposed to resolve the issue of lack of training data using mobile phones and discriminate robust feature vectors. Developing protocol to collect large training data for human activity recognition project is very tedious and may result to privacy violations. Therefore, the study generated synthetic data to augment the training sensor data generated using mobile phone. Moreover, the developed fusion of mixture density networks and long short term memory will help to reduce reliance on real training data for evaluation of deep learning.

Alzantot、Chakraborty、Srivastava(2017)在近期的研究中，探索了混合密度网络与长、短期记忆融合生成人工活动数据的方法。
针对移动电话训练数据缺乏的问题，提出了一种鲁棒特征向量识别方法。
开发针对人类活动识别项目的大规模训练数据采集协议非常繁琐，可能会导致隐私侵犯。
因此，本研究生成合成数据，对使用手机生成的训练传感器数据进行扩充。
此外，混合密度网络和长、短期记忆的融合将有助于减少对真实训练数据的依赖，以评估深度学习。},
}

